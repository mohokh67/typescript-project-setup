{
  "compilerOptions": {
    "target": "es2016",
    "esModuleInterop": true,
    "module": "CommonJS",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    "noEmitOnError": true,
    "removeComments": true,
  }
}
